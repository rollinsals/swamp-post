function updf(c)
if c.lv<1 then
 --del(foes,c)
 --del(ents,c)
 --dust(c.x,c.y)
 c.stun=true
 c.lv=c.lvmx
 c.stuntmr=45
if(c.stuntmr<=0) c.stun=false
if(c.stun) c.stuntmr-=1 return
if c.sn!=nil then
  if c.tmr%8<4 then c.left=true
  else c.left=false end
end
end

seedf(c)
if c.movesd==5 then attackf(c)
else
movef(c)
anim(c,c.sf,c.alen,3)
end

c.tmr-=1

if p.ifrm<1 then
if(col(c,p))p.lv-=1 p.ifrm=30
end
end

function movef(c)
local dx=0
local dy=0

  if(c.movesd==0)dy+=c.spd
  if(c.movesd==1)dx+=c.spd
  if(c.movesd==2)dy-=c.spd
  if(c.movesd==3)dx-=c.spd
  if(c.movesd==4)dx=0 dy=0


  c.x,c.y=move(c,dx,dy)

  if(c.x>boundx)c.movesd=3
  if(c.y>boundy)c.movesd=2
  if(c.x<0)c.movesd=1
  if(c.y<0)c.movesd=0


end

function seedf(c)
  local n=5
  if(c.at==true) n=6
  if c.tmr<=0 then
    c.movesd=flr(rnd(n))
    c.tmr=c.tmrmax
  end
end

function attackf(e)
  e.sprt=e.atspr
  bz={(e.x*8)+34,(e.y*8)+24,e.atsiz,e.atcol}
  bz.tm=5

  if p.ifrm<1 then
    local zone={
      x=e.x,
      y=e.y,
      hx=0,
      hy=0,
      hw=e.hw+e.atsiz,
      hh=e.hh+e.atsiz
   }
  if(col(zone,p))p.lv-=1 p.ifrm=30
  end

  add(particl,bz)
end




