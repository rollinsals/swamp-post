---playeraction-----------------

function smooch(dr)
	local k=ent(p.x,p.y)
	k.sprt=11
	k.spd=.1
	k.dx=0
	k.dy=0
	k.life=10
	if dr==1 then k.dy+=k.spd
	elseif dr==3 then k.dy-=k.spd
	elseif p.left==true then k.dx-=k.spd
	elseif p.left==false then k.dx+=k.spd
	end
	add(kissz,k)
	return k
end

function kisupd(k)
 if(k.life<1) del(kissz,k) del(ents,k)
 k.x,k.y=move(k,k.dx,k.dy)
 k.life-=1
 for n in all(foes) do
  if(col(k,n)) n.lv-=1 del(kissz,k) del(ents,k)
 end
end


particl={}

function updpart(pa)
 if(pa.tm<=0)del(particl,pa)
 pa.tm-=1
end

