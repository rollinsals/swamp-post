function upditm(i)
anim(i,i.sf,i.alen,3)
if i.timr!=nil then
 if i.timr>0 then i.timr-=1
 else del(items,i) end
end

if i.name==key then
  if col(i,p) then
    keys+=1
    itmset(i)
    del(items,i)
  end

elseif i.name==flwr then
  if col(i,p) then
    flwrs+=1
    itmset(i)
    del(items,i)
  end

elseif i.name==lvpotion then
  if col(i,p) then
    if(p.lv<7)p.lv+=1
    itmset(i)
    del(items,i)
  end

elseif i.name=='jet' then

  if i.jtmr <=0  and i.jeton==false then
    i.jeton=true
    i.jtmr=30
  elseif i.jtmr<=0 and i.jeton then
    i.jeton=false
    i.jtmr=i.jtmrmax
  end
  i.jtmr-=1
  if i.jeton then
    anim(i,i.sf2,i.alen,3)
    if p.ifrm<=0 then
    if col(i,p) then
      p.lv-=1
      p.ifrm=45
    end
  end
  end
end
end

