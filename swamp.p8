function _init()
--title=true
swami()
for f in all(friendslst) do
  f.kissd=false
end
palt(0,false)
palt(14,true)
end

function _update()
updp()
foreach(kissz,kisupd)
foreach(foes,updf)
foreach(friends,updfrds)
foreach(items,upditm)
foreach(particl,updpart)
end

function _draw()
cls()
--if title then title()
--else
palt(0,false)
map(mapx,mapy,30,20,8,8)
if p.ifrm%8<4 then
drawe(p)
end

foreach(foes,drawe)
foreach(friends,drawe)
foreach(kissz,drawe)
foreach(items,drawe)

for pa=1,#particl,1 do
 local px=particl[pa]
    circ(px[1],px[2],px[3],px[4])
end


drawlv()
color(2)
spr(9,6,23)
print(":"..keys,15,25)
spr(13,6,34)
print(":"..flwrs,15,36)
print("\136",58,3)
line(0,10,126,10)
if(ledger)drawfrnds()
--end
end

